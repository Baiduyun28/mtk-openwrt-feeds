/dts-v1/;
#include "mt7986b-rfb.dtsi"
/ {
	model = "MediaTek MT7986b RFB";
	compatible = "mediatek,mt7986b-nor-rfb";
};

&spi0 {
	status = "okay";
	spi_nor@0 {
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@00000 {
				label = "BL2";
				reg = <0x00000 0x0040000>;
			};
			partition@40000 {
				label = "u-boot-env";
				reg = <0x40000 0x0010000>;
			};
			factory: partition@50000 {
				label = "Factory";
				reg = <0x50000 0x00B0000>;
			};
			partition@100000 {
				label = "FIP";
				reg = <0x100000 0x0080000>;
			};
			partition@180000 {
				label = "firmware";
				reg = <0x180000 0xE00000>;
			};
		};
	};
};

&snand {
	status = "disabled";
};

&mmc0 {
	status = "disabled";
};

&mdio {
	switch@0 {
		ports {
			port@4 {
				reg = <4>;
				label = "lan4";
			};

			port@5 {
				reg = <5>;
				label = "lan5";
				phy-mode = "2500base-x";

				fixed-link {
					speed = <2500>;
					full-duplex;
					pause;
				};
			};
		};
	};
};
